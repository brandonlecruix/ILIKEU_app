{
	"name": "I LIKE U", 
	"version": "1.0", 
	"description": "Download images & videos from Instagram, see your unfollowers, autolike posts from user or hashtags.", 
	"manifest_version": 2, 
	"author": "Brandon Lacruz",
  "permissions": [
    "webRequest",
    "https://www.instagram.com/*",
    "activeTab"
  ],
  "background": {
      "scripts": ["background.js"],
      "persistent": true
  },
  "icons": {
  	"16": "icons/16.png",
  	"48": "icons/48.png",
  	"128": "icons/128.png"
  },
  "web_accessible_resources": [
      "test/*",
      "css/*",
      "fonts/*",
      "assets/*",
      "samples/*",
      "js/*"
  ], 
  "browser_action": {
      "default_icon": "icons/16.png"
  }
}
